A=[1 3 5;-2 4 6; 5 4 -8];
[eigenvecteur eigenvalue] = eig(A)

W=[0 0 ; 1 0 ; 0 1];
w=[1 0 0]';
y=[0 0]';
[vecteur iter] = NR([0.5 0]',w ,W ,A) 
K=[0 0]';
n=500;
X=linspace(-10, 10, n);
Y=linspace(-10, 10, n);
G=zeros(n,n);

for i=1:n
    for j=1:n
        y=[X(i) Y(j)]';
        K = (W'*A*(w+W*y) - w'*A*(w+W*y)*y);
        G(i,j)=K(1);        
    end
end

L=zeros(n,n);
mesh(X, Y, G); hold on;
mesh(X , Y, L);
%surface(X, Y, G)
















